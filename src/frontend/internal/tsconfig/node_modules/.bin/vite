#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/github/Ncp.Admin/frontend/node_modules/.pnpm/vite@7.3.0_@types+node@25.0_06e6984169a35df6d53d79c0ee61239c/node_modules/vite/bin/node_modules:/mnt/d/github/Ncp.Admin/frontend/node_modules/.pnpm/vite@7.3.0_@types+node@25.0_06e6984169a35df6d53d79c0ee61239c/node_modules/vite/node_modules:/mnt/d/github/Ncp.Admin/frontend/node_modules/.pnpm/vite@7.3.0_@types+node@25.0_06e6984169a35df6d53d79c0ee61239c/node_modules:/mnt/d/github/Ncp.Admin/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/github/Ncp.Admin/frontend/node_modules/.pnpm/vite@7.3.0_@types+node@25.0_06e6984169a35df6d53d79c0ee61239c/node_modules/vite/bin/node_modules:/mnt/d/github/Ncp.Admin/frontend/node_modules/.pnpm/vite@7.3.0_@types+node@25.0_06e6984169a35df6d53d79c0ee61239c/node_modules/vite/node_modules:/mnt/d/github/Ncp.Admin/frontend/node_modules/.pnpm/vite@7.3.0_@types+node@25.0_06e6984169a35df6d53d79c0ee61239c/node_modules:/mnt/d/github/Ncp.Admin/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
