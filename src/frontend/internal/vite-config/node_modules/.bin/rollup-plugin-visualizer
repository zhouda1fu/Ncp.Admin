#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/rollup-plugin-visualizer@5.14.0_rollup@4.54.0/node_modules/rollup-plugin-visualizer/dist/bin/node_modules:/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/rollup-plugin-visualizer@5.14.0_rollup@4.54.0/node_modules/rollup-plugin-visualizer/dist/node_modules:/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/rollup-plugin-visualizer@5.14.0_rollup@4.54.0/node_modules/rollup-plugin-visualizer/node_modules:/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/rollup-plugin-visualizer@5.14.0_rollup@4.54.0/node_modules:/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/rollup-plugin-visualizer@5.14.0_rollup@4.54.0/node_modules/rollup-plugin-visualizer/dist/bin/node_modules:/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/rollup-plugin-visualizer@5.14.0_rollup@4.54.0/node_modules/rollup-plugin-visualizer/dist/node_modules:/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/rollup-plugin-visualizer@5.14.0_rollup@4.54.0/node_modules/rollup-plugin-visualizer/node_modules:/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/rollup-plugin-visualizer@5.14.0_rollup@4.54.0/node_modules:/mnt/d/github/Ncp.Admin/src/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup-plugin-visualizer/dist/bin/cli.js" "$@"
else
  exec node  "$basedir/../rollup-plugin-visualizer/dist/bin/cli.js" "$@"
fi
